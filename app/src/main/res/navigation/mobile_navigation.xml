<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_oscilloscope">

    <fragment
        android:id="@+id/navigation_oscilloscope"
        android:name="com.example.pfemobile.ui.oscilloscope.OscilloscopeFragment"
        android:label="@string/title_oscilloscope"
        tools:layout="@layout/fragment_oscilloscope" />

    <fragment
        android:id="@+id/navigation_source"
        android:name="com.example.pfemobile.ui.source.SourceFragment"
        android:label="@string/title_source"
        tools:layout="@layout/fragment_source" />

    <fragment
        android:id="@+id/navigation_generator"
        android:name="com.example.pfemobile.ui.notifications.GeneratorFragment"
        android:label="@string/title_generator"
        tools:layout="@layout/fragment_generator" />
</navigation>